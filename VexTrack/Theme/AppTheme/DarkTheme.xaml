<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Color x:Key="Mono1">#838383</Color>
	<Color x:Key="Mono2">#707b83</Color>
	
	<SolidColorBrush x:Key="Background" Color="{StaticResource DarkBG}"/>
	<SolidColorBrush x:Key="Shade" Color="{StaticResource DarkShade}"/>
	<SolidColorBrush x:Key="Foreground" Color="{StaticResource DarkFG}"/>
	<SolidColorBrush x:Key="GraphIdealPoint" Color="{StaticResource Mono1}"/>

	<LinearGradientBrush x:Key="AccMono">
		<GradientStop Color="{StaticResource Mono1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Mono2}" Offset="1.0"/>
	</LinearGradientBrush>
</ResourceDictionary>