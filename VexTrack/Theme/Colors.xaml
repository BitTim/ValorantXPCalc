		<GradientStop Color="{StaticResource Purple1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Purple2}" Offset="1.0"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="AccHotCold">
		<GradientStop Color="{StaticResource Red1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Blue1}" Offset="1.0"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="AccCyberpunk">
		<GradientStop Color="{StaticResource Cyberpunk1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Cyberpunk2}" Offset="1.0"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="AccCyberpunk2">
		<GradientStop Color="{StaticResource Cyberpunk3}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Cyberpunk4}" Offset="1.0"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="AccLavender">
		<GradientStop Color="{StaticResource Lavender1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Lavender2}" Offset="1.0"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="AccAqua">
		<GradientStop Color="{StaticResource Aqua1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Aqua2}" Offset="1.0"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="AccNature">
		<GradientStop Color="{StaticResource Nature1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Nature2}" Offset="1.0"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="AccEmerald">
		<GradientStop Color="{StaticResource Emerald1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Emerald2}" Offset="1.0"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="AccChocolate">
		<GradientStop Color="{StaticResource Chocolate1}" Offset="0.0"/>
		<GradientStop Color="{StaticResource Chocolate2}" Offset="1.0"/>
	</LinearGradientBrush>



	<LinearGradientBrush x:Key="AutoBGBrush">
		<GradientStop Color="{StaticResource LightBG}" Offset="0.49"/>
		<GradientStop Color="{StaticResource DarkBG}" Offset="0.51"/>
	</LinearGradientBrush>

	<SolidColorBrush x:Key="DarkBGBrush" Color="{StaticResource DarkBG}"/>
	<SolidColorBrush x:Key="LightBGBrush" Color="{StaticResource LightBG}"/>

	

	<LinearGradientBrush x:Key="HueColors" StartPoint="0, 0" EndPoint="0, 1">
		<GradientStop Color="#ff0000" Offset="0.0"/>
		<GradientStop Color="#ff0000" Offset="0.04"/>
		<GradientStop Color="#ffff00" Offset="0.19"/>
		<GradientStop Color="#00ff00" Offset="0.35"/>
		<GradientStop Color="#00ffff" Offset="0.5"/>
		<GradientStop Color="#0000ff" Offset="0.65"/>
		<GradientStop Color="#ff00ff" Offset="0.81"/>
		<GradientStop Color="#ff0000" Offset="0.96"/>
		<GradientStop Color="#ff0000" Offset="1.0"/>
	</LinearGradientBrush>
</ResourceDictionary>